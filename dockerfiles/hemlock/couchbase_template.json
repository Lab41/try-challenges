{
    "template" : "*",
    "order" : 10,
    "mappings" : {
        "_default_" : {
            "_source" : {
                "includes" : ["doc.*", "meta.*"]
            },
            "data_detection": false,
            "dynamic_templates": [
                {
                    "store_no_index": {
                        "match": "*",
                        "mapping": {
                            "store" : "no",
                            "index" : "no",
                            "include_in_all" : false
                        }
                    }
                }
            ],
            "doc.hemlock-system": {
                "type": "string",
                "index": "not_analyzed"
            },
            "date_formats" : ["yyyy-MM-dd HH:mm:ss", "yyyy-MM-dd", "dd-MM-yyyy", "date_optional_time"],
            "properties" : {
                "meta" : {
                    "type" : "object",
                    "include_in_all" : false
                }
            }
        }
    }
}
